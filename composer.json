{
    "name": "djktechco/laravel-ide-macros",
    "description": "Fork of tutorigo/laravel-ide-macros with updated support for Laravel 11.",
    "keywords": [
        "laravel",
        "autocomplete",
        "ide",
        "macros",
        "helper",
        "phpstorm",
        "netbeans",
        "sublime",
        "codeintel",
        "phpdoc"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lyle Kuk",
            "email": "lkuk@djktech.co"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": "^5.0|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Tutorigo\\LaravelMacroHelper\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tutorigo\\LaravelMacroHelper\\IdeMacrosServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
